    "@ladjs/graceful": "^2.0.1",
    "@popperjs/core": "^2.11.5",
    "@tauri-apps/api": "^1.0.0-rc.4",
    "bootstrap-dark-5": "^1.1.3",
    "bootstrap-icons": "^1.8.1",
    "bree": "^8.0.3",
    "cabin": "^9.1.2",
    "commander": "^9.2.0",
    "koa": "^2.13.4",
    "koa-sse-stream": "^0.2.0",
    "pinia": "^2.0.13",
    "source-map-support": "^0.5.21",
    "superagent": "^7.1.3",
    "vue": "^3.2.33",
    "vue-router": "^4.0.14"
  },
  "devDependencies": {
    "@commitlint/cli": "latest",
    "@commitlint/config-conventional": "latest",
    "@prettier/plugin-pug": "^2.0.0",
    "@tauri-apps/cli": "^1.0.0-rc.9",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vue/eslint-config-prettier": "^7.0.0",
    "ava": "latest",
    "codecov": "latest",
    "concurrently": "^7.1.0",
    "cross-env": "latest",
    "esbuild": "^0.14.38",
    "eslint": "latest",
    "eslint-config-xo-lass": "latest",
    "eslint-plugin-vue": "^8.2.0",
    "execa": "^6.1.0",
    "fixpack": "latest",
    "glob": "^8.0.1",
    "husky": "latest",
    "lint-staged": "latest",
    "nyc": "latest",
    "pkg": "^5.6.0",
    "pug": "^3.0.2",
    "remark-cli": "latest",
    "remark-preset-github": "latest",
    "sass": "^1.51.0",
    "vite": "^2.9.5",
    "xo": "latest"
  },
  "homepage": "https://github.com/breejs/bree-desktop",
  "keywords": [
    "bree-desktop",
    "lass"
  ],
  "license": "MIT",
  "main": "src/main.js",
  "pkg": {
    "asset": [
      "dist/node/**/*"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/breejs/bree-desktop"
  },
  "scripts": {
    "build": "concurrently -p \"[{time}-{name}]\" -c cyan,magenta \"yarn run build:*\"",
    "build:package": "yarn package",
    "build:vue": "vite build",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "dev": "cross-env NODE_ENV=development concurrently -p \"[{time} {name}]\" -c cyan,magenta \"yarn run dev:*\"",
    "dev:package": "yarn package",
    "dev:vue": "vite",
    "lint": "yarn run lint:js && yarn run lint:md",
    "lint:js": "xo",
    "lint:md": "remark . -qfo",
    "package": "node scripts/build.mjs",
    "prepare": "husky install",
    "pretest": "yarn run lint",
    "preview": "vite preview --port 5050",
    "start:dev": "tauri dev",
    "test": "cross-env NODE_ENV=test ava",
    "test-coverage": "cross-env NODE_ENV=test nyc yarn run test"
  }
}
